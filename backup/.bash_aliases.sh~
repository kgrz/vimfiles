alias vit="echo -ne '\033]0;Vim\007'"
alias vi="vim.gnome"
# for the crtl + s
# zsh
alias vi="stty stop '' -ixoff ; vim_function"
# `Frozing' tty, so after any command terminal settings will be restored
#ttyctl -f
vim_function()
{
    local STTYOPTS="$(stty --save)"
    stty stop '' -ixoff
    command vim.gnome "$@"
    stty "$STTYOPTS"
}



