#!/usr/bin/env bash
# Tinmarino command switch
#

source "$(dirname "${BASH_SOURCE[0]}")/_shellutil.sh"

register_subcommand doc "$bin_path/tin_doc" 
register_subcommand doc "$bin_path/tin" 

doc(){
  `# Subcommand Print Documentation`
  "$bin_path/tin_doc" "$@"
}

args(){
  `# Debug: Print args passed`
  echo "Full command: $0 $*"
  i=0
  for var in "$@"; do
    echo "Arg $i: $var!"
    ((i++))
  done
}

sync(){
  `# Test sync`
  echo called sync
}

fzf(){
  `# Test fzf`
  echo called fzf
}

get_fct_dic
bin_path=$(bin_path)
call_fct_arg "$@"
# vim:sw=2:ts=2:
