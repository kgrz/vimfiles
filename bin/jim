register_subcommand misc "$bin_path/lib/class/misc.sh"

sync(){
  `# ${cblue}Sinconizacion$cend: de la clase`
  pushd ~/.vim || exit 2
  git pull --rebase \
    && echo -e "${cgreen}[+] Synchronization SUCCESS$cend" \
    || echo -e "${cred}[-] Synchronization ERROR$cend" \
  popd || exit 2
}


get_fct_dic
call_fct_arg "$@"
