bite(){ a=" alpha          bravo           charlie        delta              eco            india          jack            kili              lima            mama            nov            oscar         paris " 
b="RCS2-2111_21.18 RCS2-2143_21.85 RCS2-2338_23.60 RCS2-0047_4381    RCS2-0310_2399  RCS2-0047_2527 RCS2-0047_0.95 RCS2-2143_21.58   RCS2-2143_21.79 RCS2-2329_5295 RCS2-2327_0204 RCS2-2338_23.48 RCS2-0047_18.79"
c="3 3 5 3 2 3 5 3 3 3 3 5 5"
IFS=' ' read -a a <<< "$a"
IFS=' ' read -a b <<< "$b"
IFS=' ' read -a c <<< "$c"
for ((i=0;i<${#a[@]};i++)) ; do echo ${a[$i]},${b[$i]} ; done
}
alias myname=bite
alias topcat="bash /iraf/iraf/vo/java/topcat"


####################EXTRACT 
function extract () {
   if [ -f $1 ] ; then
       case $1 in
           *.tar.bz2)   tar xvjf $1    ;;
           *.tar.gz)    tar xvzf $1    ;;
           *.bz2)       bunzip2 $1     ;;
           *.rar)       unrar x $1       ;;
           *.gz)        gunzip $1      ;;
           *.tar)       tar xvf $1     ;;
           *.tbz2)      tar xvjf $1    ;;
           *.tgz)       tar xvzf $1    ;;
           *.zip)       unzip $1       ;;
           *.Z)         uncompress $1  ;;
           *.7z)        7z x $1        ;;
           *)           echo "don't know how to extract '$1'..." ;;
       esac
   else
       echo "'$1' is not a valid file!"
   fi
 }

alias clip="xclip -selection c"
alias c="clip"
