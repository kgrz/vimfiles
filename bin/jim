# shellcheck disable=SC2154  # cblue is referenced but not

source "$(dirname "${BASH_SOURCE[0]}")/_shellutil.sh"

bin_path=$(bin_path)
register_subcommand class011 "$bin_path/lib/class/class011.sh"
register_subcommand class012 "$bin_path/lib/class/class012.sh"
register_subcommand class013 "$bin_path/lib/class/class013.sh"
register_subcommand class021 "$bin_path/lib/class/class021.sh"
register_subcommand class022 "$bin_path/lib/class/class022.sh"
register_subcommand class031 "$bin_path/lib/class/class031.sh"
register_subcommand class032 "$bin_path/lib/class/class032.sh"
register_subcommand class041 "$bin_path/lib/class/class041.sh"
register_subcommand class042 "$bin_path/lib/class/class042.sh"
register_subcommand class051 "$bin_path/lib/class/class051.sh"
register_subcommand class052 "$bin_path/lib/class/class052.sh"
register_subcommand class053 "$bin_path/lib/class/class053.sh"


register_subcommand class061 "$bin_path/lib/class/class061.sh"
register_subcommand class062 "$bin_path/lib/class/class062.sh"
register_subcommand class071 "$bin_path/lib/class/class071.sh"
register_subcommand class072 "$bin_path/lib/class/class072.sh"
